    "# The SIR model differential equations.\n",
    "def eqn(y, t, P, beta, gamma, alpha):\n",
    "    S, I, R = y\n",
    "    dSdt = mui - (beta * S * I / P) - theta*S\n",
    "    dIdt = beta * S * I / P - theta*I - alpha*I- gamma * I \n",
    "    dRdt = gamma * I - theta*R\n",
    "    return dSdt, dIdt, dRdt"
   ]
